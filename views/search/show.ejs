<%- include('../partials/header') %>

<nav>
  <a href="/search">Search</a>
  |
  <a href="/show/new">Add Quote</a>
</nav>

<h1>Most Recent Searches</h1>

<!-- list of recent searches -->
<div id="recent">
  <% quotes.forEach(function(q) { %>
  <div class="show">
    <ul class="recent">
      <li><a href="/show/<%= q.id %>"><%= q.quote %></a></li>
      <li><%= q.actor.name %></li>
      <li><%= q.movie.title %></li>
    </ul>
  </div>
  <% }); %>
</div>
<%- include('../partials/footer') %>
